         "login" : "JeremyRand",
         "organizations_url" : "https://api.github.com/users/JeremyRand/orgs",
         "received_events_url" : "https://api.github.com/users/JeremyRand/received_events",
         "repos_url" : "https://api.github.com/users/JeremyRand/repos",
